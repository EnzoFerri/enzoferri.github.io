  <script>
  $(function() {
    $( "#accordion" )
      .accordion({ header: "> div > h3", heightStyle: "content", collapsible: true, active: "0" })
      .sortable({ axis: "y", handle: "h3", stop: function( event, ui ) {
          ui.item.children( "h3" ).triggerHandler( "focusout" );
        }
      });
  });
  </script>

<title>Accordion</title>
</head>
